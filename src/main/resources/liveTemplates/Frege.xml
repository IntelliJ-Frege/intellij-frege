<templateSet group="Frege">
  <template name="class" value="class $CLASS_DEF$ where&#10;    $func1$ = $expr1$&#10;    $END$" description="Create new class" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_DEF" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="func1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="expr1" expression="" defaultValue="&quot;undefined&quot;" alwaysStopAt="true" />
    <context>
      <option name="FREGE_CLASSDECL" value="true" />
    </context>
  </template>
  <template name="ife" value="if $BOOLEAN_EXPR$ then $IF_TRUE$ else $IF_FALSE$" description="Create if then else conditional" toReformat="false" toShortenFQNames="true">
    <variable name="BOOLEAN_EXPR" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="IF_TRUE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="IF_FALSE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="FREGE_IF" value="true" />
    </context>
  </template>
</templateSet>